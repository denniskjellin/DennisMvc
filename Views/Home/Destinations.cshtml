@{
    ViewData["Title"] = "Destinations";
}

<div class="container">
    <div class="col-1">
        <section>
        <h1>Destinations</h1>
        <p>@ViewBag.Destinations</p>
        </section>

        <section></br>
         <h2>Visited</h2>
            <ul>
            @foreach (var destination in Model)
            {
                <li class="li-items"><a target="_blanc" href="@destination.DestinationUrl">@destination.DestinationName</a></li>
            }
            </ul>    
    </section>
        
    </div>
    
</div>