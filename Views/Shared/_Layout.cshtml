<!DOCTYPE html>
<html lang="en">
  <head>
     <title>@ViewData["Title"]</title>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="~/css/app.css" />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Montserrat:wght@400;500;600;700;800&family=Poppins&display=swap"
      rel="stylesheet"
    />
  </head>
  <body>
    <header>
      <div class="header">
       <a id="logo-container" asp-controller="Home" asp-action="Index"> <img alt="DennisMvc" class="logo"  src="~/images/logo.svg"></a>
      </div>
    </header>
    <div class="wrap">
      <div class="toggleBtn">
        <div class="hamburger">
          <span></span>
        </div>
      </div>
      <aside class="sidebar">
        <h3>Menu</h3>
@await Html.PartialAsync("_NavbarPartial")
      </aside>
        <main>
            @RenderBody()
        </main>
      </div>
    <script src="~/js/app.js"></script>
    @await Html.PartialAsync("_FooterPartial")
    </div>
  </body>
</html>
